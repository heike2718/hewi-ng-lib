{
  "name": "hewi-ng-lib",
  "version": "5.1.1",
  "description": "custom angular components like modal to be used in my angular projects",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build hewi-ng-lib",
    "copy-license": "copyfiles --flat ./LICENSE dist/hewi-ng-lib",
    "copy-readme": "copyfiles --flat ./README.md dist/hewi-ng-lib",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm-pack": "cd dist/hewi-ng-lib && npm pack",
    "package": "npm run build-lib && npm run copy-files && npm run npm-pack"
  },
  "private": false,
  "dependencies": {
